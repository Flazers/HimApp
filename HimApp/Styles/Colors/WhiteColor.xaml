<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Basic" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Hover" Color="#D1D1D1"/>
    <SolidColorBrush x:Key="Pressed" Color="#E6E6E6"/>
    <SolidColorBrush x:Key="Disabled" Color="#858585"/>

    <Color x:Key="BasicColor">#FFFFFF</Color>
    <Color x:Key="HoverColor">#D1D1D1</Color>
    <Color x:Key="PressedColor">#E6E6E6</Color>
    <Color x:Key="DisabledColor">#858585</Color>
</ResourceDictionary>